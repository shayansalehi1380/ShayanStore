@{
    List<Domain.Entity.City> city = ViewBag.Cities;
}

<div class="card">
    <div class="card-header">
        <h3 class="card-title">لیست شهر و استان</h3>
    </div>
    <!-- /.card-header -->
    <div class="card-body p-0">
        <table class="table table-striped">
            <thead>
                <tr>
                    <th style="width: 10px">#</th>
                    <th>شهر</th>
                    <th>استان</th>
                    <th style="width: 40px">عملیات</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in city)
                {
                    <tr>
                        <td>@item.Id</td>
                        <td>@item.Name</td>
                        @* <td>@item.State.Name</td> *@
                    </tr>
                }
            </tbody>
        </table>
    </div>
    <!-- /.card-body -->
</div>